@{
    Layout = null;
}


@*Contact Index Sayfası*@

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("HeadPartial")

<body>
    <div class="container-xxl bg-white p-0">
        <!-- Spinner Start -->
        @await Component.InvokeAsync("SpinnerPartial")
        <!-- Spinner End -->
        <!-- Header Start -->
        @await Component.InvokeAsync("NavbarPartial")
        <!-- Header End -->
        <!-- Carousel Start -->
        @await Component.InvokeAsync("ContactCoverPartial")

        <!-- Carousel End -->
        <!-- Booking Start -->
        @await Component.InvokeAsync("ReservationPartial")
        <!-- Booking End -->
        @await Html.PartialAsync("/Views/Contact/SendMessage.cshtml")
        @*        @await Component.InvokeAsync("BookingAddPartial")*@

        <!-- Newsletter Start -->
        @await Component.InvokeAsync("SubscribePartial")
        <!-- Newsletter Start -->
        <!-- Footer Start -->
        @await Component.InvokeAsync("FooterPartial")
        <!-- Footer End -->
        <!-- Back to Top -->
        <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>
    </div>
    @await Component.InvokeAsync("ScriptPartial")
</body>

</html>
